{
  "name": "lepto",
  "version": "1.0.0",
  "description": "Advanced images minification CLI tool",
  "main": "src/lepto.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimitrinicolas/lepto.git"
  },
  "keywords": [
    "front-end",
    "images",
    "optimization",
    "javascript",
    "jpg",
    "webpg"
  ],
  "author": "Dimitri NICOLAS <dimitri@fivecorp.fr>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dimitrinicolas/lepto/issues"
  },
  "homepage": "https://github.com/dimitrinicolas/lepto",
  "dependencies": {
    "chokidar": "^2.0.3",
    "deep-object-diff": "^1.1.0",
    "directory-tree": "^2.1.0",
    "exec-buffer": "^3.2.0",
    "fast-glob": "^2.2.2",
    "file-type": "^8.0.0",
    "fs-extra": "^6.0.1",
    "gifsicle": "^3.0.4",
    "image-size": "^0.6.2",
    "minimatch": "^3.0.4",
    "pngquant": "^1.3.0",
    "sharp": "^0.20.3",
    "socket.io": "^2.1.1",
    "svgo": "^1.0.5"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "css-mqpacker": "^6.0.2",
    "focus-visible": "^4.1.4",
    "glob": "^7.0.5",
    "npm-watch": "^0.3.0",
    "postcss": "^6.0.22",
    "postcss-axis": "^0.1.2",
    "postcss-calc": "^6.0.0",
    "postcss-cli": "^5.0.0",
    "postcss-color-function": "^4.0.1",
    "postcss-import": "^11.1.0",
    "postcss-inline-media": "^1.0.1",
    "postcss-nested": "^3.0.0",
    "postcss-position": "^1.0.0",
    "postcss-pxtorem": "^4.0.0",
    "postcss-simple-vars": "^4.1.0",
    "postcss-size": "^2.0.0",
    "sort-css-media-queries": "^1.3.4",
    "uglify-js": "^3.4.0",
    "webpack": "^4.11.1",
    "webpack-cli": "^3.0.3"
  },
  "watch": {
    "style:build": {
      "patterns": [
        "gui/src"
      ],
      "extensions": "css"
    },
    "script:build": {
      "patterns": [
        "gui/src"
      ],
      "extensions": "js"
    }
  },
  "scripts": {
    "style:build": "postcss --config gui/build/postcss.config.js -o gui/dist/style.css gui/src/main.css",
    "script:build": "webpack gui/src/main.js --output-path='./gui/dist/' --output-filename='script.js' --config gui/build/webpack.config.js",
    "browser-sync": "browser-sync start --config gui/build/bs.config.js",
    "browser-sync-silent": "browser-sync start --config gui/build/bs.config.js --no-open",
    "start": "npm-watch"
  }
}
