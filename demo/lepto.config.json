{
  "input": "demo/input",
  "output": "demo/output",
  "watch": true,
  "watchConfig": true,
  "followUnlink": true,
  "gui": true,
  "openGui_": true,
  "dataOutput": "demo/output/data.json",
  "filters": [
    {
      "glob": "**/*.*",
      "use": [
        {
          "name": "lepto.jpeg",
          "quality": 75,
          "forceExt": "jpg"
        },
        {
          "name": "lepto.png",
          "compression": 9
        },
        {
          "name": "lepto.gif",
          "colors": 100,
          "optimizationLevel": 3,
          "interlaced": true
        },
        {
          "name": "lepto.svg"
        }
      ]
    },
    {
      "glob": [
        "**/*.{jpg,jpeg}",
        "**/*.yo"
      ],
      "use": [
        {
          "name": "../lepto-resize",
          "maxWidth": 1200
        },
        {
          "name": "../lepto-webp",
          "quality": 80
        },
        {
          "name": "../lepto-vibrant-color"
        }
      ]
    },
    {
      "glob": "**/*.png",
      "use": [
        {
          "name": "../lepto-resize",
          "maxSize": 64,
          "retina": [
            2,
            3
          ]
        }
      ]
    },
    {
      "glob": "photos/001.jpg",
      "use": [
        {
          "name": "lepto.jpeg",
          "quality": 1
        }
      ]
    },
    {
      "glob": "photos/003.jpeg",
      "use": [
        {
          "name": "lepto.jpeg",
          "quality": 16
        }
      ]
    },
    {
      "glob": "midi-controller-css.gif",
      "use": [
        {
          "name": "lepto.gif",
          "colors": 255
        }
      ]
    }
  ],
  "testGui": 0.05825242752895199
}
