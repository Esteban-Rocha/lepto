{
  "input": "assets/input",
  "output": "assets/output",
  "watch": true,
  "use": [
    {
      "name": "lepto.jpeg",
      "quality": 80
    },
    {
      "name": "lepto.png",
      "compression": 9
    }
  ],
  "filters": [
    {
      "glob": "**/*.png",
      "use": [
        {
          "name": "lepto.resize",
          "retina": [
            2,
            3
          ]
        }
      ]
    }
  ]
}