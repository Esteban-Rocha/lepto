{
  "watch": true,
  "input": "./demo/input",
  "output": "./demo/output",
  "filters": [
    {
      "glob": ["**/*.{jpg,jpeg}"],
      "use": [
        {
          "name": "lepto.webp"
        },
        {
          "name": "lepto.resize",
          "width": 800
        }
      ]
    },
    {
      "glob": "**/*.png",
      "use": [
        {
          "name": "lepto.resize",
          "width": 32
        }
      ]
    }
  ]
}
